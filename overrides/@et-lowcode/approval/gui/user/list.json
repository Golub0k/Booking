{
  "@title": "Реестр пользователей",
  "type": "object",
  "widget": {
    "name": "container",
    "events": {
      "init": {
        "$session-storage-set": {
          "key": "userBackLink",
          "value": "list"
        }
      }
    }
  },
  "model": "/",
  "properties": {
    "breadcrumbs": {
      "type": "object",
      "widget": {
        "name": "breadcrumb",
        "mobileItems": [
          {
            "label": "Главная",
            "routerLink": "/cabinet/event/list"
          }
        ],
        "items": [
          {
            "label": "Главная",
            "routerLink": "/cabinet/event/list"
          },
          {
            "label": "Реестр пользователей"
          }
        ],
        "class": {
          "group": "p-xs-auto mrg-lg-b-xl mrg-b-md"
        }
      }
    },
    "header-with-button": {
      "type": "object",
      "widget": {
        "name": "container",
        "options": {
          "label": "Реестр пользователей"
        },
        "class": {
          "group": "p-grid mrg-b-lg mrg-t-0",
          "label": "h1 p-xs-12 p-xl-9 pdd-tb-0",
          "value": "p-xs-12 p-xl-3 pdd-tb-0 p-jc-xl-end p-as-center p-autolayout"
        }
      },
      "properties": {
        "button": {
          "type": "string",
          "widget": {
            "name": "button",
            "options": {
              "label": "Добавить пользователя",
              "visible": {
                "$api-workflow-check-action": {
                  "workflow": "user",
                  "action": "add"
                }
              }
            },
            "events": {
              "click": {
                "$go": {
                  "workflow": "user",
                  "action": "add"
                }
              }
            },
            "class": {
              "value": "button_primary button_narrow button_h1"
            }
          }
        }
      }
    },
    "search_with_mobile_filters": {
      "type": "object",
      "widget": {
        "name": "container",
        "class": {
          "group": "mrg-b-md search-with-filters",
          "value": "p-autolayout"
        }
      },
      "properties": {
        "search": {
          "model": "#search",
          "type": "string",
          "widget": {
            "name": "input-group-button",
            "options": {
              "placeholder": "Фамилия, имя, отчество, логин, e-mail",
              "actionButtonIconMobile": "icon-search",
              "actionButtonLabel": "Найти",
              "enableClearButton": true
            },
            "events": {
              "actionButtonClick": {
                "$if": {
                  "condition": {
                    "$validate": {
                      "path": "/"
                    }
                  },
                  "then": {
                    "$grid": {
                      "action": "lazyLoad",
                      "path": "#data",
                      "filters": {
                        "$assign-object": {
                          "object": {
                            "$var": {
                              "path": "#desktop-filter"
                            }
                          },
                          "data": {
                            "$var": {
                              "path": "#search"
                            }
                          },
                          "path": "search"
                        }
                      }
                    }
                  },
                  "else": {}
                }
              },
              "keypress-13": {
                "$if": {
                  "condition": {
                    "$validate": {
                      "path": "/"
                    }
                  },
                  "then": {
                    "$grid": {
                      "action": "lazyLoad",
                      "path": "#data",
                      "filters": {
                        "$assign-object": {
                          "object": {
                            "$var": {
                              "path": "#desktop-filter"
                            }
                          },
                          "data": {
                            "$var": {
                              "path": "#search"
                            }
                          },
                          "path": "search"
                        }
                      }
                    }
                  },
                  "else": {}
                }
              }
            },
            "class": {
              "group": "form-item form-item_last height-fit-content flex-1 d-xl-flex",
              "value": "form-item-element",
              "actionButtonGroup": "button_outline"
            }
          }
        },
        "filters-button-mobile": {
          "widget": {
            "name": "button",
            "options": {
              "icon": "icon-filters",
              "visible": {
                "$is-between": {
                  "value": {
                    "$to-pixel": {
                      "value": {
                        "$get-window-breakpoint": {
                          "subscriptionMode": true
                        }
                      }
                    }
                  },
                  "upper_value": {
                    "$to-pixel": {
                      "value": "xl"
                    }
                  }
                }
              }
            },
            "events": {
              "click": {
                "$element-option": {
                  "option": "visible",
                  "path": "#modalMobileSearch",
                  "value": true
                }
              }
            },
            "class": {
              "group": "mrg-l-xs",
              "value": "button_icon button_icon-shaded p-fluid w-48"
            }
          }
        }
      }
    },
    "table": {
      "value": {
        "$api-table-workflow-data": {
          "model": "#data",
          "query": "user/list",
          "withState": true,
          "variables": {
            "$graphql-event-variables": {
              "data": {
                "$element-event": {
                  "event": "lazyLoad"
                }
              }
            }
          },
          "errors": {
            "default": {
              "$message": {
                "severity": "error",
                "summary": "Ошибка",
                "detail": "При загрузке данных произошла ошибка, повторите попытку позднее"
              }
            }
          }
        }
      },
      "type": "array",
      "model": "#data",
      "widget": {
        "name": "table-lazy",
        "paginatorLeft": {
          "type": "string",
          "widget": "info",
          "value": {
            "$grid": {
              "action": "rows-per-page",
              "path": "#data"
            }
          }
        },
        "options": {
          "emptyMessage": "Данные не найдены",
          "responsive": false,
          "first": 0,
          "lazy": true,
          "paginator": true,
          "rows": 10,
          "rowsPerPageOptions": [
            10,
            20,
            50
          ],
          "routerOptions": [
            "first",
            "rows"
          ],
          "initialFilters": {
            "$var": {
              "path": "#desktop-filter"
            }
          },
          "sortField": "lastName",
          "sortOrder": 1
        },
        "class": {
          "group": "p-xxl-9 p-xs-12"
        },
        "events": {
          "lazyLoad": {
            "$element-option": {
              "path": "#data",
              "option": "loading",
              "value": true
            }
          }
        },
        "subscriptions": {
          "reload-table": {
            "$element-event": {
              "event": "reload",
              "value": true
            }
          }
        }
      },
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "email": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "lastName": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "firstName": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "middleName": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "userName": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "ФИО",
                "sortable": "lastName"
              }
            },
            "properties": {
              "fullName": {
                "type": "string",
                "value": {
                  "$replace": {
                    "template": "<span class=\"user-name\">{{0}}<br/><span class=\"fwn\">{{1}}{{2}}</span></span>",
                    "data": [
                      {
                        "$from-context": {
                          "path": "$rowData.lastName"
                        }
                      },
                      {
                        "$from-context": {
                          "path": "$rowData.firstName"
                        }
                      },
                      {
                        "$if": {
                          "condition": {
                            "$not-empty": {
                              "data": {
                                "$from-context": {
                                  "path": "$rowData.middleName"
                                }
                              }
                            }
                          },
                          "then": {
                            "$array-join": {
                              "array": {
                                "$array-concat": {
                                  "data": {
                                    "$from-context": {
                                      "path": "$rowData.middleName"
                                    }
                                  },
                                  "array": [
                                    " "
                                  ]
                                }
                              },
                              "separator": ""
                            }
                          },
                          "else": ""
                        }
                      }
                    ]
                  }
                },
                "widget": {
                  "name": "html",
                  "class": {
                    "value": "user-state_offline"
                  }
                }
              }
            }
          },
          "login": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Логин",
                "sortable": true
              }
            }
          },
          "phone": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            },
            "properties": {
              "ext": {
                "type": "string",
                "widget": {
                  "name": "hidden"
                }
              },
              "prefix": {
                "type": "string",
                "widget": {
                  "name": "hidden"
                }
              },
              "num": {
                "type": "string",
                "widget": {
                  "name": "hidden"
                }
              }
            }
          },
          "contacts": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Контактные данные"
              }
            },
            "properties": {
              "fullName": {
                "type": "string",
                "value": {
                  "$replace": {
                    "template": "{{0}}<br/><div>{{1.prefix}} {{1.num}}",
                    "data": [
                      {
                        "$from-context": {
                          "path": "$rowData.email"
                        }
                      },
                      {
                        "$if": {
                          "condition": {
                            "$not-empty": {
                              "data": {
                                "$from-context": {
                                  "path": "$rowData.phone.num"
                                }
                              }
                            }
                          },
                          "then": {
                            "$from-context": {
                              "path": "$rowData.phone"
                            }
                          },
                          "else": {
                            "ext": "",
                            "prefix": "",
                            "num": ""
                          }
                        }
                      },
                      {
                        "$if": {
                          "condition": {
                            "$not-empty": {
                              "data": {
                                "$fetch-object": {
                                  "path": "ext",
                                  "data": {
                                    "$from-context": {
                                      "path": "$rowData.phone"
                                    }
                                  }
                                }
                              }
                            }
                          },
                          "then": {
                            "$replace": {
                              "template": "доб: {{0}} ",
                              "data": [
                                {
                                  "$from-context": {
                                    "path": "$rowData.phone.ext"
                                  }
                                }
                              ]
                            }
                          },
                          "else": ""
                        }
                      }
                    ]
                  }
                },
                "widget": {
                  "name": "html",
                  "class": {
                    "value": "flex-column"
                  }
                }
              }
            }
          },
          "organization": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            },
            "properties": {
              "anyName": {
                "type": "string",
                "widget": {
                  "name": "hidden"
                }
              },
              "state": {
                "type": "string",
                "widget": {
                  "name": "hidden"
                }
              }
            }
          },
          "organizationId": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "organizationNameView": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Организация",
                "visible": false
              }
            },
            "properties": {
              "nameText": {
                "type": "string",
                "widget": {
                  "name": "info",
                  "options": {
                    "visible": {
                      "$not": {
                        "data": {
                          "$active-user-has-permission": {
                            "permission": {
                              "$replace": {
                                "template": "organization.view.{{0}}",
                                "data": [
                                  {
                                    "$from-context": {
                                      "path": "$rowData.organization.state"
                                    }
                                  }
                                ]
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "value": {
                  "$from-context": {
                    "path": "$rowData.organization.anyName"
                  }
                }
              },
              "nameLink": {
                "type": "string",
                "widget": {
                  "name": "link",
                  "options": {
                    "routerLink": "/cabinet/organization/view",
                    "queryParams": {
                      "id": {
                        "$from-context": {
                          "path": "$rowData.organizationId"
                        }
                      }
                    },
                    "visible": {
                      "$active-user-has-permission": {
                        "permission": {
                          "$replace": {
                            "template": "organization.view.{{0}}",
                            "data": [
                              {
                                "$from-context": {
                                  "path": "$rowData.organization.state"
                                }
                              }
                            ]
                          }
                        }
                      }
                    }
                  }
                },
                "value": {
                  "$from-context": {
                    "path": "$rowData.organization.anyName"
                  }
                }
              }
            }
          },
          "roles": {
            "type": "array",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            },
            "items": {
              "type": "object",
              "properties": {
                "role": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "widget": "hidden"
                    }
                  },
                  "widget": {
                    "name": "fieldset"
                  }
                }
              },
              "widget": {
                "name": "fieldset"
              }
            }
          },
          "roleList": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Роли"
              }
            },
            "properties": {
              "roles": {
                "type": "array",
                "widget": {
                  "name": "info",
                  "options": {
                    "separator": "<br>",
                    "asList": "true",
                    "viewedValue": {
                      "$array-map": {
                        "array": {
                          "$subs": {
                            "path": "#data[].roles"
                          }
                        },
                        "map": {
                          "$from-context": {
                            "path": "$data.role.name"
                          }
                        }
                      }
                    }
                  }
                },
                "items": {
                  "type": "object",
                  "properties": {
                    "role": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "widget": "info"
                        }
                      },
                      "widget": {
                        "name": "fieldset"
                      }
                    }
                  },
                  "widget": {
                    "name": "fieldset"
                  }
                }
              }
            }
          },
          "userAD": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "userAD_ru": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Добавлен",
                "visible": true
              }
            },
            "value": {
              "$switch": {
                "condition": {
                  "$subs": {
                    "path": "#data[].userAD"
                  }
                },
                "default": "Вручную",
                "cases": [
                  {
                    "value": true,
                    "action": "Active Directory"
                  },
                  {
                    "value": false,
                    "action": "Вручную"
                  }
                ]
              }
            }
          },
          "blockReason": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            },
            "properties": {
              "reason": {
                "type": "string",
                "widget": "hidden"
              }
            }
          },
          "state": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "statusName": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Статус"
              }
            },
            "properties": {
              "state": {
                "type": "string",
                "value": {
                  "$replace": {
                    "template": "<span class=\"badge badge_{{colorCode}}\">{{name}}</span>{{additionalInfo}}",
                    "data": {
                      "colorCode": {
                        "$switch": {
                          "condition": {
                            "$subs": {
                              "path": "#data[].state"
                            }
                          },
                          "default": "",
                          "cases": [
                            {
                              "value": "await_email_confirmation",
                              "action": "draft"
                            },
                            {
                              "value": "blocked",
                              "action": "blocked"
                            },
                            {
                              "value": "active",
                              "action": "active"
                            }
                          ]
                        }
                      },
                      "name": {
                        "$switch": {
                          "condition": {
                            "$subs": {
                              "path": "#data[].state"
                            }
                          },
                          "default": "",
                          "cases": [
                            {
                              "value": "await_email_confirmation",
                              "action": "Ожидает подтверждения e-mail"
                            },
                            {
                              "value": "blocked",
                              "action": "Заблокирован"
                            },
                            {
                              "value": "active",
                              "action": "Активен"
                            }
                          ]
                        }
                      },
                      "additionalInfo": {
                        "$if": {
                          "condition": {
                            "===": {
                              "operand1": {
                                "$from-context": {
                                  "path": "$rowData.state"
                                }
                              },
                              "operand2": "blocked"
                            }
                          },
                          "then": {
                            "$replace": {
                              "template": "<div class=\"text-secondary mrg-t-xs\"><b>Причина:</b><br/>{{statusReason}}</div>",
                              "data": {
                                "statusReason": {
                                  "$if": {
                                    "condition": {
                                      "$from-context": {
                                        "path": "$rowData.userAD"
                                      }
                                    },
                                    "then": "Учетная запись Active Directory заблокирована",
                                    "else": {
                                      "$encode-tag": {
                                        "string": {
                                          "$from-context": {
                                            "path": "$rowData.blockReason.reason"
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          },
                          "else": ""
                        }
                      }
                    }
                  }
                },
                "widget": {
                  "name": "html",
                  "class": {
                    "value": "p-flex-column p-ai-start"
                  }
                }
              }
            }
          },
          "actions_column": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Действия"
              },
              "class": {
                "header": "column_actions",
                "cell": "column_actions"
              }
            },
            "properties": {
              "actions": {
                "type": "array",
                "model": "actions",
                "widget": {
                  "name": "actions-button",
                  "items": {
                    "$subs": {
                      "path": "#data[].actions"
                    }
                  }
                },
                "items": {
                  "type": "action",
                  "properties": {
                    "id": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "title": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "icon": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "url": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "routerLink": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "popup": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "hidden": {
                      "type": "string",
                      "widget": "hidden"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "desktop-filter": {
      "type": "object",
      "model": "#desktop-filter",
      "widget": {
        "name": "container",
        "options": {
          "visible": {
            "$is-between": {
              "value": {
                "$to-pixel": {
                  "value": {
                    "$get-window-breakpoint": {
                      "subscriptionMode": true
                    }
                  }
                }
              },
              "lower_value": {
                "$to-pixel": {
                  "value": "xxl"
                }
              }
            }
          }
        },
        "class": {
          "group": "p-xxl-3 filters-block d-xxl-flex d-none",
          "value": "filters-block_wrapper d-block"
        }
      },
      "properties": {
        "header": {
          "model": false,
          "type": "object",
          "widget": {
            "name": "container",
            "class": {
              "value": "p-autolayout p-jc-between p-ai-center"
            }
          },
          "properties": {
            "label": {
              "model": false,
              "type": "string",
              "widget": {
                "name": "info",
                "options": {
                  "viewedValue": "Фильтры"
                },
                "class": {
                  "value": "h3"
                }
              }
            },
            "clear-button": {
              "widget": {
                "name": "button",
                "options": {
                  "label": "Очистить"
                },
                "class": {
                  "value": "button_link"
                },
                "events": {
                  "click": [
                    {
                      "$populate": {
                        "path": "#desktop-filter",
                        "data": {}
                      }
                    },
                    {
                      "$populate": {
                        "path": "#mobile-filter",
                        "data": {}
                      }
                    },
                    {
                      "$grid": {
                        "action": "lazyLoad",
                        "path": "#data",
                        "filters": {}
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "content": {
          "model": false,
          "type": "object",
          "widget": {
            "name": "accordion",
            "options": {
              "multiple": true
            },
            "class": {
              "group": "filters-block_accordion"
            }
          },
          "properties": {
