{
  "@title": "Реестр помещений",
  "type": "object",
  "widget": "container",
  "model": "/",
  "properties": {
    "breadcrumbs": {
      "type": "object",
      "widget": {
        "name": "breadcrumb",
        "mobileItems": [
          {
            "label": "Главная",
            "routerLink": "/cabinet/event/list"
          }
        ],
        "items": [
          {
            "label": "Главная",
            "routerLink": "/cabinet/event/list"
          },
          {
            "label": "Реестр помещений"
          }
        ],
        "class": {
          "group": "p-xs-auto mrg-lg-b-xl mrg-b-md"
        }
      }
    },
    "header-with-button": {
      "type": "object",
      "widget": {
        "name": "container",
        "options": {
          "label": "Реестр помещений"
        },
        "kind": "container-header-with-button-container-style"
      },
      "properties": {
        "button": {
          "type": "string",
          "widget": {
            "name": "button",
            "options": {
              "label": "Добавить помещение",
              "visible": {
                "$api-workflow-check-action": {
                  "workflow": "room",
                  "action": "add"
                }
              }
            },
            "events": {
              "click": {
                "$go": {
                  "workflow": "room",
                  "action": "add"
                }
              }
            },
            "class": {
              "value": "button_primary button_narrow button_h1"
            }
          }
        }
      }
    },
    "search": {
      "model": "#search",
      "type": "object",
      "widget": {
        "name": "container",
        "class": {
          "group": "mrg-b-md search-with-filters",
          "value": "p-autolayout"
        }
      },
      "properties": {
        "search": {
          "type": "string",
          "widget": {
            "name": "input-group-button",
            "options": {
              "placeholder": "Наименование, номер, тип помещения",
              "actionButtonIconMobile": "icon-search",
              "actionButtonLabel": "Найти",
              "enableClearButton": true
            },
            "events": {
              "actionButtonClick": {
                "$if": {
                  "condition": {
                    "$validate": {
                      "path": "/"
                    }
                  },
                  "then": {
                    "$grid": {
                      "action": "lazyLoad",
                      "path": "#data",
                      "filters": {
                        "$var": {
                          "path": "#search"
                        }
                      }
                    }
                  },
                  "else": {}
                }
              },
              "keypress-13": {
                "$if": {
                  "condition": {
                    "$validate": {
                      "path": "/"
                    }
                  },
                  "then": {
                    "$grid": {
                      "action": "lazyLoad",
                      "path": "#data",
                      "filters": {
                        "$var": {
                          "path": "#search"
                        }
                      }
                    }
                  },
                  "else": {}
                }
              }
            },
            "class": {
              "group": "form-item form-item_last height-fit-content flex-1 d-xl-flex",
              "value": "form-item-element",
              "actionButtonGroup": "button_outline"
            }
          }
        },
        "filters-button-mobile": {
          "widget": {
            "name": "button",
            "options": {
              "icon": "icon-filters",
              "visible": {
                "$is-between": {
                  "value": {
                    "$to-pixel": {
                      "value": {
                        "$get-window-breakpoint": {
                          "subscriptionMode": true
                        }
                      }
                    }
                  },
                  "upper_value": {
                    "$to-pixel": {
                      "value": "xl"
                    }
                  }
                }
              }
            },
            "events": {
              "click": {
                "$element-option": {
                  "option": "visible",
                  "path": "#modalMobileSearch",
                  "value": true
                }
              }
            },
            "class": {
              "group": "mrg-l-xs",
              "value": "button_icon button_icon-shaded p-fluid w-48"
            }
          }
        }
      }
    },
    "table": {
      "value": {
        "$api-table-workflow-data": {
          "model": "#data",
          "query": "room/list",
          "withState": true,
          "variables": {
            "$graphql-event-variables": {
              "data": {
                "$element-event": {
                  "event": "lazyLoad"
                }
              }
            }
          },
          "errors": {
            "default": {
              "$message": {
                "severity": "error",
                "summary": "Ошибка",
                "detail": "При загрузке данных произошла ошибка, повторите попытку позднее"
              }
            }
          }
        }
      },
      "type": "array",
      "model": "#data",
      "widget": {
        "name": "table-lazy",
        "paginatorLeft": {
          "type": "string",
          "widget": "info",
          "value": {
            "$grid": {
              "action": "rows-per-page",
              "path": "#data"
            }
          }
        },
        "options": {
          "emptyMessage": "Данные не найдены",
          "responsive": false,
          "first": 0,
          "lazy": true,
          "paginator": true,
          "rows": 10,
          "rowsPerPageOptions": [
            10,
            20,
            50
          ],
          "routerOptions": [
            "first",
            "rows"
          ],
          "sortField": "name",
          "sortOrder": 1
        },
        "class": {
          "group": "p-xxl-9 p-xs-12"
        },
        "events": {
          "lazyLoad": {
            "$element-option": {
              "path": "#data",
              "option": "loading",
              "value": true
            }
          }
        },
        "subscriptions": {
          "reload-table": {
            "$element-event": {
              "event": "reload",
              "value": true
            }
          }
        }
      },
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "name": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Наименование",
                "sortable": true
              }
            }
          },
          "roomType": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Тип"
              }
            },
            "properties": {
              "name": {
                "type": "string",
                "widget": {
                  "name": "info"
                }
              }
            }
          },
          "number": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Номер",
                "visible": true
              }
            }
          },
          "floor": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Этаж",
                "visible": false
              }
            }
          },
          "floorView": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Этаж",
                "visible": true
              }
            },
            "value": {
              "$if": {
                "condition": {
                  "===": {
                    "operand1": {
                      "$subs": {
                        "path": "#data[].floor"
                      }
                    },
                    "operand2": "open_area"
                  }
                },
                "then": "Открытая выставочная площадь",
                "else": {
                  "$var": {
                    "path": "#data[].floor"
                  }
                }
              }
            }
          },
          "square": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "squareView": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Площадь (м²)",
                "visible": true,
                "formatter": "number:'0.2-2':'ru'"
              }
            },
            "properties": {
              "square": {
                "type": "string",
                "widget": {
                  "name": "info",
                  "options": {
                    "formatter": "number:'0.2-2':'ru'"
                  },
                  "class": {
                    "value": "p-jc-end"
                  }
                },
                "value": {
                  "$from-context": {
                    "path": "$rowData.square"
                  }
                }
              }
            }
          },
          "roominess": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "roominessView": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Вместительность (чел.)",
                "visible": true
              }
            },
            "properties": {
              "roominess": {
                "type": "string",
                "widget": {
                  "name": "info",
                  "options": {
                    "formatter": "number:'1.0-0':'ru'"
                  },
                  "class": {
                    "value": "p-jc-end"
                  }
                },
                "value": {
                  "$from-context": {
                    "path": "$rowData.roominess"
                  }
                }
              }
            }
          },
          "configuration": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "transformation": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "configuration_ru": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Конфигурация",
                "visible": true
              }
            },
            "value": {
              "$switch": {
                "condition": {
                  "$subs": {
                    "path": "#data[].configuration"
                  }
                },
                "default": "",
                "cases": [
                  {
                    "value": true,
                    "action": "Да"
                  },
                  {
                    "value": false,
                    "action": "Нет"
                  }
                ]
              }
            }
          },
          "transformation_ru": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Трансформация",
                "visible": true
              }
            },
            "value": {
              "$switch": {
                "condition": {
                  "$subs": {
                    "path": "#data[].transformation"
                  }
                },
                "default": "",
                "cases": [
                  {
                    "value": true,
                    "action": "Да"
                  },
                  {
                    "value": false,
                    "action": "Нет"
                  }
                ]
              }
            }
          },
          "state": {
            "type": "string",
            "widget": {
              "name": "table-column",
              "options": {
                "visible": false
              }
            }
          },
          "statusName": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Статус"
              }
            },
            "properties": {
              "state": {
                "type": "string",
                "value": {
                  "$replace": {
                    "template": "<span class=\"badge badge_{{colorCode}}\">{{name}}</span>",
                    "data": {
                      "colorCode": {
                        "$switch": {
                          "condition": {
                            "$subs": {
                              "path": "#data[].state"
                            }
                          },
                          "default": "",
                          "cases": [
                            {
                              "value": "archived",
                              "action": "blocked"
                            },
                            {
                              "value": "active",
                              "action": "active"
                            }
                          ]
                        }
                      },
                      "name": {
                        "$switch": {
                          "condition": {
                            "$subs": {
                              "path": "#data[].state"
                            }
                          },
                          "default": "Активно",
                          "cases": [
                            {
                              "value": "archived",
                              "action": "В архиве"
                            },
                            {
                              "value": "active",
                              "action": "Активно"
                            }
                          ]
                        }
                      }
                    }
                  }
                },
                "widget": {
                  "name": "html",
                  "class": {
                    "value": "p-flex-column p-ai-start"
                  }
                }
              }
            }
          },
          "actions_column": {
            "type": "object",
            "widget": {
              "name": "table-column",
              "options": {
                "label": "Действия"
              },
              "class": {
                "header": "column_actions",
                "cell": "column_actions"
              }
            },
            "properties": {
              "actions": {
                "type": "array",
                "model": "actions",
                "widget": {
                  "name": "actions-button",
                  "items": {
                    "$subs": {
                      "path": "#data[].actions"
                    }
                  }
                },
                "items": {
                  "type": "action",
                  "properties": {
                    "id": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "title": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "icon": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "url": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "routerLink": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "popup": {
                      "type": "string",
                      "widget": "hidden"
                    },
                    "hidden": {
                      "type": "string",
                      "widget": "hidden"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "desktop-filter": {
      "type": "object",
      "model": "#desktop-filter",
      "widget": {
        "name": "container",
        "options": {
          "visible": {
            "$is-between": {
              "value": {
                "$to-pixel": {
                  "value": {
                    "$get-window-breakpoint": {
                      "subscriptionMode": true
                    }
                  }
                }
              },
              "lower_value": {
                "$to-pixel": {
                  "value": "xxl"
                }
              }
            }
          }
        },
        "class": {
          "group": "p-xxl-3 filters-block d-xxl-flex d-none",
          "value": "filters-block_wrapper d-block"
        }
      },
      "properties": {
        "header": {
          "model": false,
          "type": "object",
          "widget": {
            "name": "container",
            "class": {
              "value": "p-autolayout p-jc-between p-ai-center"
            }
          },
          "properties": {
            "label": {
              "model": false,
              "type": "string",
              "widget": {
                "name": "info",
                "options": {
                  "viewedValue": "Фильтры"
                },
                "class": {
                  "value": "h3"
                }
              }
            },
            "clear-button": {
              "widget": {
                "name": "button",
                "options": {
                  "label": "Очистить"
                },
                "class": {
                  "value": "button_link"
                },
                "events": {
                  "click": [
                    {
                      "$populate": {
                        "path": "#desktop-filter",
                        "data": {}
                      }
                    },
                    {
                      "$populate": {
                        "path": "#mobile-filter",
                        "data": {}
                      }
                    },
                    {
                      "$grid": {
                        "action": "lazyLoad",
                        "path": "#data",
                        "filters": {}
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "content": {
          "model": false,
          "type": "object",
          "widget": {
            "name": "accordion",
            "options": {
              "multiple": true
            },
            "class": {
              "group": "filters-block_accordion"
            }
          },
          "properties": {
            "type": {
              "type": "object",
              "model": false,
              "widget": {
                "name": "accordion-tab",
                "options": {
                  "label": "Тип помещения",
                  "selected": true
                }
              },
              "properties": {
                "type": {
                  "type": "any",
                  "widget": {
                    "name": "multiselect",
                    "options": {
                      "placeholder": "Выбрать",
                      "useEllipsisWhenDisplayFieldOverflows": false,
                      "filter": true,
                      "showClear": true,
                      "optionLabel": "name",
                      "valueKey": "id",
                      "dataKey": "id",
                      "valueOptions": {
                        "$fetch-object": {
                          "path": "data.items",
                          "data": {
                            "$api-data": {
                              "query": "room_type/get_for_dropdown"
                            }
                          }
                        }
                      }
                    }
                  },
                  "model": "#desktop-filter.type"
                }
              }
            },
            "floor": {
              "type": "object",
              "model": false,
              "widget": {
                "name": "accordion-tab",
                "options": {
                  "label": "Этаж",
                  "selected": true
                }
              },
              "properties": {
                "floor": {
                  "type": "any",
                  "widget": {
                    "name": "checkbox",
                    "options": {
                      "valueKey": "code",
                      "optionLabel": "title",
                      "isMulticheckbox": true,
                      "valueOptions": [
                        {
                          "code": "open_area",
                          "title": "Открытая выставочная площадь"
                        },
                        {
                          "code": "1",
                          "title": "1 этаж"
                        },
                        {
                          "code": "1,5",
                          "title": "1,5 этаж"
                        },
                        {
                          "code": "2",
                          "title": "2 этаж"
                        }
                      ]
                    },
                    "class": {
                      "value": "flex-column filter-block_checkbox"
                    }
                  },
                  "model": "#desktop-filter.floor"
                }
              }
            },
            "square": {
              "model": false,
              "type": "object",
              "widget": {
                "name": "accordion-tab",
                "options": {
                  "label": "Площадь (м²)",
                  "selected": true
                }
              },
              "properties": {
                "squareFrom": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "От",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 8,
                        "allowMinus": false,
                        "digits": 2
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                },
                "divider": {
                  "type": "string",
                  "model": false,
                  "value": "—",
                  "widget": {
                    "name": "info",
                    "class": {
                      "group": "p-xl-2",
                      "value": "text-medium p-jc-center p-ai-center"
                    }
                  }
                },
                "squareTo": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "До",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 8,
                        "allowMinus": false,
                        "digits": 2
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                }
              }
            },
            "roominess": {
              "model": false,
              "type": "object",
              "widget": {
                "name": "accordion-tab",
                "options": {
                  "label": "Вместительность (чел.)",
                  "selected": true
                }
              },
              "properties": {
                "roominessFrom": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "От",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 10,
                        "allowMinus": false,
                        "digits": 0
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                },
                "divider": {
                  "type": "string",
                  "model": false,
                  "value": "—",
                  "widget": {
                    "name": "info",
                    "class": {
                      "group": "p-xl-2",
                      "value": "text-medium p-jc-center p-ai-center"
                    }
                  }
                },
                "roominessTo": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "До",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 10,
                        "allowMinus": false,
                        "digits": 0
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                }
              }
            }
          }
        },
        "search-button": {
          "widget": {
            "name": "button",
            "options": {
              "label": "Применить"
            },
            "class": {
              "group": "p-autolayout p-jc-end",
              "value": "button_narrow button_outline button_clear"
            },
            "events": {
              "click": {
                "$if": {
                  "condition": {
                    "$validate": {
                      "path": "/"
                    }
                  },
                  "then": {
                    "$waterfall": {
                      "arrayFunctions": [
                        {
                          "$populate": {
                            "path": "#mobile-filter",
                            "data": {
                              "$var": {
                                "path": "#desktop-filter"
                              }
                            }
                          }
                        },
                        {
                          "$grid": {
                            "action": "lazyLoad",
                            "path": "#data",
                            "filters": {
                              "$assign-object": {
                                "object": {
                                  "$var": {
                                    "path": "#desktop-filter"
                                  }
                                },
                                "data": {
                                  "$var": {
                                    "path": "#search.search"
                                  }
                                },
                                "path": "search"
                              }
                            }
                          }
                        }
                      ]
                    }
                  },
                  "else": {}
                }
              }
            }
          }
        }
      }
    },
    "mobile-filter": {
      "type": "object",
      "model": "#modalMobileSearch",
      "widget": {
        "name": "dialog",
        "options": {
          "visible": false,
          "closable": true
        },
        "class": {
          "dialog": "no-radius filters-block-mobile",
          "panel": "dialog-fixed-width"
        }
      },
      "properties": {
        "content": {
          "model": "#mobile-filter",
          "type": "object",
          "widget": {
            "name": "container",
            "class": {
              "group": "height-fit-content",
              "value": "filters-block_wrapper"
            }
          },
          "properties": {
            "header": {
              "model": false,
              "type": "object",
              "widget": {
                "name": "container",
                "class": {
                  "value": "p-autolayout p-jc-between p-ai-center"
                }
              },
              "properties": {
                "label": {
                  "model": false,
                  "type": "string",
                  "widget": {
                    "name": "info",
                    "options": {
                      "viewedValue": "Фильтры"
                    },
                    "class": {
                      "value": "h3 mrg-b-lg"
                    }
                  }
                }
              }
            },
            "type": {
              "type": "any",
              "widget": {
                "name": "multiselect",
                "options": {
                  "label": "Тип помещения",
                  "placeholder": "Выбрать",
                  "useEllipsisWhenDisplayFieldOverflows": false,
                  "filter": true,
                  "showClear": true,
                  "optionLabel": "name",
                  "valueKey": "id",
                  "dataKey": "id",
                  "valueOptions": {
                    "$fetch-object": {
                      "path": "data.items",
                      "data": {
                        "$api-data": {
                          "query": "room_type/get_for_dropdown"
                        }
                      }
                    }
                  }
                },
                "kind": "multiselect-search-form-expanded-input-full-line-style"
              }
            },
            "floor": {
              "type": "any",
              "widget": {
                "name": "checkbox",
                "options": {
                  "label": "Этаж",
                  "valueKey": "code",
                  "optionLabel": "title",
                  "isMulticheckbox": true,
                  "valueOptions": [
                    {
                      "code": "open_area",
                      "title": "Открытая выставочная площадь"
                    },
                    {
                      "code": "1",
                      "title": "1 этаж"
                    },
                    {
                      "code": "1,5",
                      "title": "1,5 этаж"
                    },
                    {
                      "code": "2",
                      "title": "2 этаж"
                    }
                  ]
                },
                "class": {
                  "group": "form-item pdd-b-0 p-col-12",
                  "label": "form-label form-label_top",
                  "value": "flex-column filter-block_checkbox"
                }
              }
            },
            "square": {
              "model": false,
              "type": "object",
              "widget": {
                "name": "container",
                "options": {
                  "label": "Площадь (м²)"
                },
                "kind": "container-search-form-expanded-input-full-style"
              },
              "properties": {
                "squareFrom": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "От",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 8,
                        "allowMinus": false,
                        "digits": 2
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                },
                "divider": {
                  "type": "string",
                  "model": false,
                  "value": "—",
                  "widget": {
                    "name": "info",
                    "class": {
                      "group": "p-xl-2",
                      "value": "text-medium p-jc-center p-ai-center"
                    }
                  }
                },
                "squareTo": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "До",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 8,
                        "allowMinus": false,
                        "digits": 2
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                }
              }
            },
            "roominess": {
              "model": false,
              "type": "object",
              "widget": {
                "name": "container",
                "options": {
                  "label": "Вместительность (чел.)"
                },
                "kind": "container-search-form-expanded-input-full-style"
              },
              "properties": {
                "roominessFrom": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "От",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 10,
                        "allowMinus": false,
                        "digits": 0
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                },
                "divider": {
                  "type": "string",
                  "model": false,
                  "value": "—",
                  "widget": {
                    "name": "info",
                    "class": {
                      "group": "p-xl-2",
                      "value": "text-medium p-jc-center p-ai-center"
                    }
                  }
                },
                "roominessTo": {
                  "type": "number",
                  "bindRouter": true,
                  "widget": {
                    "name": "input",
                    "options": {
                      "placeholder": "До",
                      "inputmask": {
                        "alias": "integer",
                        "groupSeparator": " ",
                        "autoGroup": true,
                        "radixPoint": ",",
                        "integerDigits": 10,
                        "allowMinus": false,
                        "digits": 0
                      }
                    },
                    "class": {
                      "group": "p-xl-5"
                    }
                  }
                }
              }
            },
            "buttons": {
              "type": "object",
              "widget": {
                "name": "container",
                "kind": "container-search-form-panel-buttons-center-style"
              },
              "properties": {
                "search-button": {
                  "widget": {
                    "name": "button",
                    "options": {
                      "label": "Применить"
                    },
                    "events": {
                      "click": {
                        "$if": {
                          "condition": {
                            "$validate": {
                              "path": "/"
                            }
                          },
                          "then": {
                            "$waterfall": {
                              "arrayFunctions": [
                                {
                                  "$populate": {
                                    "path": "#desktop-filter",
                                    "data": {
                                      "$var": {
                                        "path": "#mobile-filter"
                                      }
                                    }
                                  }
                                },
                                {
                                  "$element-option": {
                                    "option": "visible",
                                    "path": "#modalMobileSearch",
                                    "value": false
                                  }
                                },
                                {
                                  "$grid": {
                                    "action": "lazyLoad",
                                    "path": "#data",
                                    "filters": {
                                      "$assign-object": {
                                        "object": {
                                          "$var": {
                                            "path": "#mobile-filter"
                                          }
                                        },
                                        "data": {
                                          "$var": {
                                            "path": "#search.search"
                                          }
                                        },
                                        "path": "search"
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          },
                          "else": {}
                        }
                      }
                    }
                  }
                },
                "clear-button": {
                  "widget": {
                    "name": "button",
                    "options": {
                      "label": "Сбросить"
                    },
                    "class": {
                      "value": "button_link"
                    },
                    "events": {
                      "click": [
                        {
                          "$populate": {
                            "path": "#mobile-filter",
                            "data": {}
                          }
                        },
                        {
                          "$populate": {
                            "path": "#desktop-filter",
                            "data": {}
                          }
                        },
                        {
                          "$grid": {
                            "action": "lazyLoad",
                            "path": "#data",
                            "filters": {
                              "$assign-object": {
                                "object": {
                                  "$var": {
                                    "path": "#mobile-filter"
                                  }
                                },
                                "data": {
                                  "$var": {
                                    "path": "#search"
                                  }
                                },
                                "path": "search"
                              }
                            }
                          }
                        },
                        {
                          "$element-option": {
                            "option": "visible",
                            "path": "#modalMobileSearch",
                            "value": false
                          }
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
